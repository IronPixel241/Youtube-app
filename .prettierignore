# Ignore artifacts
build
dist
node_modules

# Ignore bundling output
*.tgz
*.zip

# Ignore environment files
.env
.env.local
.env.*
*.env

# Ignore lock files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore public files (static assets)
public/

# Ignore test coverage reports
coverage

# Ignore production files
*.min.js
*.min.css

# Ignore data files
*.log
*.csv

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore configuration files
*.config.js
*.config.json

# Ignore miscellaneous
.idea/
.vscode/
*.iml
